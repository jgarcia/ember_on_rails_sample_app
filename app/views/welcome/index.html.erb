<h1>Contacts</h1>

<script type="text/x-handlebars">
	<table class="table table-striped table-bordered">
		<tr>
			<th>Name</th>
			<th>Email</th>
			<th>Phone</th>
		</tr>
	{{#each App.contactsController}}
		{{#view App.ContactView contactBinding="this"}}
		<td>{{contact.name}}</td>
		<td>{{contact.email}}</td>
		<td>{{contact.telephone}}</td>
		{{/view}}
	{{/each}}
	</table>
</script>
